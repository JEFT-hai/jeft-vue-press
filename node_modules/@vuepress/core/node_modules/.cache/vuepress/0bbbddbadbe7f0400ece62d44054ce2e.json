nt :is=\"layout\" />\r\n    <footer><h1>Footer</h1></footer> -->\r\n    <div :class=\"['main', { showMobile: showMobile }]\">\r\n      <component :is=\"layout\" />\r\n      <div class=\"mobile\" v-if=\"showMobile\">\r\n        <iframe\r\n          ref=\"iframe\"\r\n          class=\"demo\"\r\n          :src=\"exampleUrl\"\r\n          frameborder=\"0\"\r\n        ></iframe>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      urlPrefix: \"https://jeft-hai.github.io/jeft-vue-example/#/\",\r\n      // urlPrefix: \"http://jeft-vue-example.xuhaichao.cn/#/\",\r\n      // urlPrefix: \"http://localhost:8081/#/\",\r\n    };\r\n  },\r\n  computed: {\r\n    layout() {\r\n      if (this.$page.path) {\r\n        if (this.$frontmatter.layout) {\r\n          // 你也可以像默认的 globalLayout 一样首先检测 layout 是否存在\r\n          return this.$frontmatter.layout;\r\n        }\r\n        return \"Layout\";\r\n      }\r\n      return \"NotFound\";\r\n    },\r\n    showMobile() {\r\n      return this.$page.path.indexOf(\"components\") != -1;\r\n    },\r\n    exampleUrl() {\r\n      if (!this.showMobile) {\r\n        return \"\";\r\n      } else {\r\n        const path = this.$page.path;\r\n        const base = path.replace(\".html\", \"\");\r\n        const baseName = base.split(\"/\").pop();\r\n        return this.urlPrefix + baseName;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n  .main {\r\n    /* width: 100vw;\r\n                                                                                                                                display: flex;\r\n                                                                                                                                flex-direction: row;\r\n                                                                                                                                overflow: hidden; */\r\n  }\r\n  .theme-container {\r\n    /* flex: 1; */\r\n\r\n    background-color: #f7f7f7;\r\n  }\r\n  .mobile {\r\n    position: fixed;\r\n    z-index: 1;\r\n    right: 10px;\r\n    top: 100px;\r\n    height: 540px;\r\n    width: 360px;\r\n    border-radius: 12px;\r\n    background-color: white;\r\n    overflow: hidden;\r\n  }\r\n  .main.showMobile .theme-container {\r\n    padding-right: 380px;\r\n  }\r\n\r\n  .mobile .demo {\r\n    height: 100%;\r\n    width: 100%;\r\n  }\r\n</style>"]}]}